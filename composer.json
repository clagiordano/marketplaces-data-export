{
  "name": "clagiordano/marketplaces-data-export",
  "description": "marketplaces-data-export is a library for generic data export from marketplaces",
  "type": "library",
  "license": "MIT",
  "keywords": [
      "clagiordano",
      "marketplaces",
      "data",
      "export",
      "soap",
      "api",
      "rest",
      "ebay",
      "seller",
      "amazon",
      "mws"
  ],
  "authors": [
      {
          "name": "Claudio Giordano",
          "email": "claudio.giordano@autistici.org",
          "role": "Developer"
      }
  ],
  "autoload": {
      "psr-4": {
          "clagiordano\\MarketplacesDataExport\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "clagiordano\\MarketplacesDataExport\\Tests\\": "tests/"
      }
  },
  "require": {
      "php": ">=5.6",
      "clagiordano/weblibs-configmanager": "^1.0",
      "dts/ebay-sdk-php": "^18.0",
      "mcs/amazon-mws": "^0.1.25"
  },
  "require-dev": {
      "phpunit/phpunit": "^4.8",
      "phpmetrics/phpmetrics": "^2.3",
      "phpstan/phpstan": "^0.9.1",
      "clagiordano/phpunit-result-printer": "^1.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --no-coverage",
    "test-coverage": "./vendor/bin/phpunit"
  }
}
